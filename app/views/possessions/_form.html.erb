<%= simple_form_for(@possession) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :category_id %>
    <%= f.input :title %>
    <%= f.input :user_id %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
